{
  "name": "mds-starter-theme",
  "version": "4.0.0",
  "author": "MD Silviu",
  "main": "",
  "scripts": {
    "start": "webpack --progress --watch --display-error-details --display-reasons --mode development",
    "build": "webpack --progress --mode production",
    "lint": "run-s -cl lint:scripts lint:styles",
    "lint-fix": "run-s -cl lint-fix:wpcs lint-fix:scripts lint-fix:styles",
    "lint:scripts": "eslint --color ./scripts/**",
    "lint:styles": "stylelint --color ./styles/**",
    "lint-fix:scripts": "eslint --color --fix ./scripts/**",
    "lint-fix:styles": "stylelint --color --fix ./styles/**",
    "i18n": "run-s -cl i18n:textdomain i18n:pot",
    "i18n:textdomain": "wpi18n addtextdomain --exclude=dist,docs,node_modules,vendor,.development,assets --textdomain=mds_starter_theme",
    "i18n:pot": "wpi18n makepot --exclude=dist,docs,node_modules,resources,vendor,.development,assets --main-file=../style.css --domain-path=../languages/ --pot-file=mds_starter_theme.pot --type=wp-theme --textdomain=mds_starter_theme",
    "build-archive": "node shscripts/export.js"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@wordpress/babel-plugin-import-jsx-pragma": "^2.1.0",
    "@wordpress/babel-plugin-makepot": "^3.0.0",
    "@wordpress/babel-preset-default": "^4.1.0",
    "@wordpress/eslint-plugin": "^2.1.0",
    "archiver": "^3.0.0",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.5",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "css-mqpacker": "^7.0.0",
    "css-mqpacker-sort-mediaqueries": "^0.2.2",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-wordpress": "^0.1.0",
    "file-loader": "^3.0.1",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-jpeg-recompress": "^6.0.0",
    "imagemin-pngquant": "^7.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "node-wp-i18n": "^1.2.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.6.1",
    "svg-spritemap-webpack-plugin": "^3.3.1",
    "terser-webpack-plugin": "^1.2.4",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.32.0",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "eventlistener-polyfill": "^1.0.5",
    "jquery": "^3.4.1",
    "object-fit-images": "^3.2.4",
    "svg4everybody": "^2.1.9",
    "whatwg-fetch": "^3.0.0"
  },
  "browserslist": [
    ">2%",
    "last 3 versions",
    "Firefox ESR",
    "not ie < 11"
  ]
}
